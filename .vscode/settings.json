{
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "javascriptreact",
    "html",
    "typescriptreact",
    "vue"
  ],
  "cSpell.words": [
    "dataurl"
  ],
  "filewatcher.commands": [{
    "match": "\\.md*",
    "isAsync": true,
    "cmd": "echo '${file} file content Changed'",
    "event": "onFileChange"
  }]

}